#Upper Case and Lower Case

str=input()
uc=0
lc=0
for i in str:
    if i>='A' and i<='Z':
        uc=uc+1
    elif i>='a' and i<='z':
        lc=lc+1
print(uc)
print(lc)


#Count the word and Character

str=input()
count=len(str.split())
print(count)
print(len(str))


#Number of Digits and Letters in a String

str=input()
ch=0
di=0
for i in str:
    if i>='0' and i<='9':
        di=di+1
    else:
        ch=ch+1
print("Characters=",ch)
print("Digits=",di)


